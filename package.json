{
  "name": "webhook",
  "version": "1.0.0",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "esbuild src/server.ts --bundle --outfile=build/server.js --platform=node --format=esm --packages=external",
    "start": "node build/server.js",
    "dev": "tsx watch src/server.ts",
    "generate": "drizzle-kit generate:pg",
    "migrate": "tsx src/migrate.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "consola": "^3.4.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "drizzle-orm": "^0.38.4",
    "drizzle-zod": "^0.6.1",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "pg": "^8.13.1",
    "postgres": "^3.4.5",
    "request-ip": "^3.3.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/pg": "^8.11.10",
    "@types/request-ip": "^0.0.41",
    "drizzle-kit": "^0.30.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  }
}
